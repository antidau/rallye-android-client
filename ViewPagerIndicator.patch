From a23269551f49e4541203a7a1afe1fb41a406220d Mon Sep 17 00:00:00 2001
From: Ramon Wirsch <ramon.wirsch@stud.tu-darmstadt.de>
Date: Fri, 20 Dec 2013 18:48:27 +0100
Subject: [PATCH 1/3] + gradle config

---
 library/build.gradle | 25 +++++++++++++++++++++++++
 1 file changed, 25 insertions(+)
 create mode 100644 library/build.gradle

diff --git a/library/build.gradle b/library/build.gradle
new file mode 100644
index 0000000..c23fe1a
--- /dev/null
+++ b/library/build.gradle
@@ -0,0 +1,25 @@
+apply plugin: 'android-library'
+
+dependencies {
+    compile 'com.android.support:support-v4:19.+'
+}
+
+android {
+    compileSdkVersion 19
+    buildToolsVersion '19.0.0'
+
+    defaultConfig {
+        minSdkVersion 4
+        targetSdkVersion 19
+    }
+
+    sourceSets {
+        main {
+            manifest.srcFile 'AndroidManifest.xml'
+            java.srcDirs = ['src']
+            res.srcDirs = ['res']
+        }
+    }
+}
+
+//apply from: 'https://raw.github.com/chrisbanes/gradle-mvn-push/master/gradle-mvn-push.gradle'
\ No newline at end of file
